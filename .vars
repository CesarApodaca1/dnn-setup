# Setups

export LANG=en_US.UTF-8

export EDITOR='nano';

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && . "$NVM_DIR/bash_completion"  # This loads nvm bash_completion

# Add Visual Studio Code (code)
export PATH="$PATH:/Applications/Visual Studio Code.app/Contents/Resources/app/bin"

# Flutter
export PATH="$PATH:$HOME/dev/flutter/bin"

# PODS
export GEM_HOME=$HOME/.gem
export PATH=$GEM_HOME/bin:$PATH

# AWS Pem
export AWSKEY="$HOME/.ssh/dnn-aws.pem"

# MAVEN
export MAVEN_OPTS="-Xmx8G"

# WIZELINE

export GITHUB_TOKEN="0b54ce4b5e74ced33e4fe65b757996ae03d80d02"

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/daniel.garcia/Desktop/workspace/gcloud/google-cloud-sdk/path.zsh.inc' ]; then . '/Users/daniel.garcia/Desktop/workspace/gcloud/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/daniel.garcia/Desktop/workspace/gcloud/google-cloud-sdk/completion.zsh.inc' ]; then . '/Users/daniel.garcia/Desktop/workspace/gcloud/google-cloud-sdk/completion.zsh.inc'; fi

export GCR_REPO="gcr.io/betterapp-3-284817"

# Android SDK
export ANDROID_HOME=$HOME/Library/Android/sdk
export PATH=$PATH:$ANDROID_HOME/emulator
export PATH=$PATH:$ANDROID_HOME/tools
export PATH=$PATH:$ANDROID_HOME/tools/bin export PATH=$PATH:$ANDROID_HOME/platform-tools

# SPACESHIP
SPACESHIP_DOCKER_SHOW=false
